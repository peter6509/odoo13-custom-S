<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_travel_report" model="ir.sequence">
            <field name="name">Travel Report</field>
            <field name="code">neweb_sale_analysis.travel_report</field>
            <field name="prefix">ET%(year)s%(month)s%(day)s</field>
<!--             <field name="auto_reset">True</field>-->
<!--            <field name="reset_period">day</field>-->
            <field name="padding">3</field>
<!--            <field name="reset_init_number">1</field>-->
            <field name="company_id" eval="False"/>
        </record>
         <record id="seq_expense_report" model="ir.sequence">
            <field name="name">Expense Report</field>
            <field name="code">neweb_sale_analysis.expense_report</field>
            <field name="prefix">EXP%(year)s%(month)s%(day)s</field>
<!--             <field name="auto_reset">True</field>-->
<!--            <field name="reset_period">day</field>-->
            <field name="padding">2</field>
<!--            <field name="reset_init_number">1</field>-->
            <field name="company_id" eval="False"/>
        </record>
         <record id="seq_officialdoc_report" model="ir.sequence">
            <field name="name">Official Document</field>
            <field name="code">neweb_sale_analysis.official_doc</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
<!--             <field name="auto_reset">True</field>-->
<!--            <field name="reset_period">day</field>-->
            <field name="padding">2</field>
<!--            <field name="reset_init_number">1</field>-->
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>