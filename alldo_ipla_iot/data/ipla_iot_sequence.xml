<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_ipla_iot_workorder" model="ir.sequence">
            <field name="name">IOT Workorder</field>
            <field name="code">alldo_ipla_iot.workorder</field>
            <field name="prefix">WO%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>

        </record>

         <record id="seq_ipla_iot_outsuborder" model="ir.sequence">
            <field name="name">IOT outsuborder</field>
            <field name="code">alldo_ipla_iot.outsuborder</field>
            <field name="prefix">SUB%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>

        </record>

        <record id="seq_ipla_iot_shipping" model="ir.sequence">
            <field name="name">IOT shipping</field>
            <field name="code">alldo_ipla_iot.shipping</field>
            <field name="prefix">S%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>

        </record>

        <record id="seq_ipla_iot_outsourcing_out" model="ir.sequence">
            <field name="name">IOT outsourcing out</field>
            <field name="code">alldo_ipla_iot.outsourcing_out</field>
            <field name="prefix">O%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>

        </record>

        <record id="seq_ipla_iot_return_ng" model="ir.sequence">
            <field name="name">IOT return NG</field>
            <field name="code">alldo_ipla_iot.return_ng</field>
            <field name="prefix">R%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>

        </record>
    </data>
</odoo>