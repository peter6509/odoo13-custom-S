    <data>
        <record id="view_neweb_contract_tx_price_report_pivot" model="ir.ui.view">
            <field name="name">neweb_contract.contract.tx_price.report.pivot</field>
            <field name="model">neweb_contract.contract.tx_price.report</field>
            <field name="arch" type="xml">
                <pivot string="Contracts Analysis">
                    <field name="date" type="row"/>
                    <field name="end_customer" type="row"/>
                    <field name="tx_price" type="measure"/>
                </pivot>
            </field>
        </record>
        <record id="view_neweb_contract_tx_price_report_graph" model="ir.ui.view">
            <field name="name">neweb_contract.contract.tx_price.report.graph</field>
            <field name="model">neweb_contract.contract.tx_price.report</field>
            <field name="arch" type="xml">
                <graph string="Contracts Analysis">
                    <field name="date"/>
                    <field name="tx_price" type="measure"/>
                </graph>
            </field>
        </record>
        <record id="action_neweb_contract_tx_price_report" model="ir.actions.act_window">
            <field name="name">Contracts Analysis</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">neweb_contract.contract.tx_price.report</field>
            <field name="view_mode">pivot,graph</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                <!-- Add Text Here -->
              </p>This report performs analysis on your Contracts<p>
                <!-- More details about what a user can do with this object will be OK -->
              </p>
            </field>
        </record>

