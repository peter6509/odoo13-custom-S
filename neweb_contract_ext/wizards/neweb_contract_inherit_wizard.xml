<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       <record id="views_contract_inherit_copy_wizard" model="ir.ui.view">
            <field name="name">views.contract.inherit.copy.wizard</field>
            <field name="model">neweb_contract.contract_inherit_wizard</field>
            <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <h1>合約續約COPY序號所屬第四頁籤數據</h1>
                    <group>
                      <field name="origin_contract_id" options="{'no_create_edit':True,'no_create':True}"/>
                    </group>
                </sheet>
                <footer>
                    <button type="object" name="run_inherit_copy" string="序號數據資訊複製" class="oe_highlight"/>
                    <button special="cancel" string="放棄"/>
                </footer>
            </form>
            </field>
        </record>
        <record id="menu_contract_inherit_copy_wizard_action" model="ir.actions.act_window">
             <field name="name">合約續約COPY序號所屬第四頁籤數據</field>
             <field name="res_model">neweb_contract.contract_inherit_wizard</field>
             <field name="view_mode">form</field>
             <field name="target">new</field>
         </record>

    </data>
</odoo>