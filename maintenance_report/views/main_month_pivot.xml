<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="main_month_pivot" model="ir.ui.view">
            <field name="name">main_month_pivot</field>
            <field name="model">maintenance_month.pivot</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name" />
                    <field name="department_name"/>
                    <field name="equipment_name"/>
                    <field name="equipment_desc"/>
                    <field name="maintenance_type"/>
                    <field name="brokentime"/>
                    <field name="waittime"/>
                    <field name="maintenancetime"/>
                    <field name="request_date"/>
                    <field name="process_date"/>
                    <field name="repaired_date"/>
                    <field name="technician"/>
                    <field name="faulttext"/>
                    <field name="maintenance_content"/>
                    <field name="parts_name"/>

                </tree>
            </field>
        </record>
        <record id="main_month_pivot_form" model="ir.ui.view">
            <field name="name">main_month_pivot_form</field>
            <field name="model">maintenance_month.pivot</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="department_name"/>
                            <field name="equipment_name"/>
                            <field name="equipment_desc"/>
                            <field name="maintenance_type"/>
                            <field name="brokentime"/>
                            <field name="waittime"/>
                            <field name="maintenancetime"/>
                            <field name="request_date"/>
                            <field name="process_date"/>
                            <field name="repaired_date"/>
                            <field name="technician"/>
                            <field name="faulttext"/>
                            <field name="maintenance_content"/>
                            <field name="parts_name"/>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="main_month_pivot_action" model="ir.actions.act_window">
            <field name="name">main_month_pivot_action</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">maintenance_month.pivot</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    <!-- Add Text Here -->
                </p>
                <p>
                    <!-- More details about what a user can do with this object will be OK -->
                </p>
            </field>
        </record>
        <!-- This Menu Item must have a parent and an action -->
        <!--<menuitem id="main_month_pivot_menu_action" name="Main_month_pivot" parent="" action="main_month_pivot_action" sequence=""/>-->
    </data>
</odoo>