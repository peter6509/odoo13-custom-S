<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="One2ManySelectable">
		<div>
			<!-- Demo Button that will add 'delete' button above one2many selection but if you use widget="one2many_selectable" -->
			<t t-if="!widget.get('effective_readonly')">
				<button style="margin-bottom: 1%;display:none;" class="button_delete_sale_order_lines btn btn-primary oe_highlight"
						href="javascript:void(0)">
					Delete
				</button>
			</t>
			<t t-if="widget.get('effective_readonly')">
				<span class="oe_form_char_content"></span>
			</t>
		</div>
    </t>
    
</templates>
