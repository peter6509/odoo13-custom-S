<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_era_contract" model="ir.sequence">
            <field name="name">ERA contract No</field>
            <field name="code">era.contract</field>
            <field name="prefix">C%(year)s%(month)s%(day)s</field>
            <field name="padding">2</field>
        </record>
        <record id="seq_era_landlord_contract" model="ir.sequence">
            <field name="name">ERA landlord contract No</field>
            <field name="code">era.landlord_contract</field>
            <field name="prefix">L%(year)s%(month)s%(day)s</field>
            <field name="padding">2</field>
        </record>
    </data>
</odoo>