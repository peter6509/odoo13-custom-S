<TableData name="ds2" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p"/>
<O>
<![CDATA[A]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemoTW]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT c.客戶評級,round(sum(a.單價*a.數量)/count(c.客戶ID),2) as 平均值
FROM S訂單明細 a,S訂單 b,客戶評級 c
where c.客戶評級='${p}' and c.評級日期 like '%2011%' and a.訂單ID=b.訂單ID and b.客戶ID=c.客戶ID
group by c.客戶評級
]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="ds3" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p"/>
<O>
<![CDATA[A]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemoTW]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT count(客戶ID),客戶評級 FROM 客戶評級
where 客戶評級='${p}' and 評級日期 like '%2011%'
group by 客戶評級]]></Query>
