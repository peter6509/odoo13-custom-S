<TableData name="ds4" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemoTW]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select a.客戶評級,個數1 as '2012',個數2 as '2013' from  (SELECT 客戶評級, count(客戶ID) as 個數2 FROM 客戶評級
where 評級日期 like '%2012%'
group by 客戶評級) as a
inner join 
(SELECT count(客戶ID) as 個數1,客戶評級 FROM 客戶評級
where 評級日期 like '%2011%'
group by 客戶評級) as b
on a.客戶評級=b.客戶評級]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="ds5" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemoTW]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT 公司名稱,金額,上期金額,金額-上期金額 as 差額 FROM 客戶評級
where 評級日期 like '%2011%' and 上期金額 is not null
order by 差額 desc limit 10]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="ds6" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemoTW]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select aa.客戶評級,數量,中間商數量 from (SELECT b.客戶評級,count(a.客戶ID) as 數量 FROM S客戶 a,客戶評級 b
where a.客戶ID=b.客戶ID and b.評級日期 like '%2011%'
group by b.客戶評級) as aa
inner join 
(SELECT count(a.客戶ID) as 中間商數量,b.客戶評級 FROM S客戶 a,客戶評級 b
where a.客戶ID=b.客戶ID and b.評級日期 like '%2011%' and a.是否中間商 ='true'
group by b.客戶評級) as bb
on aa.客戶評級=bb.客戶評級]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="ds7" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemoTW]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select a.客戶評級,總額,中間商總額 from (SELECT 客戶評級,round(sum(金額),2) as 總額 FROM 客戶評級 
where 評級日期 like '%2011%'
group by 客戶評級) as a
inner join 
(SELECT 客戶評級,round(sum(金額),2) as 中間商總額 FROM 客戶評級,S客戶 c
where 客戶評級.客戶ID=c.客戶ID and 評級日期 like '%2011%' and 是否中間商 ='true'
group by 客戶評級) as b
on a.客戶評級=b.客戶評級


]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
