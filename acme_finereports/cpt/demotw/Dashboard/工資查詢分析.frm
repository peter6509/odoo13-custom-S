<TableData name="Cht_Wage_Sales_Profit" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="year"/>
<O>
<![CDATA[2010]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemoTW]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT A.年度,
	工資合計,
	銷售收入,
	利潤,
	ROUND(工資合計/銷售收入,2) AS 工資銷售比,
	ROUND(工資合計/利潤,2) AS 工資利潤比
FROM 
	(
	SELECT 年度,
		SUM(合計) AS 工資合計
	FROM 崗位工資統計
	WHERE 1=1
		${if(len(year)==0,"","AND 年度="+year+"")}
	GROUP BY 年度
	) A
JOIN 
	(
	SELECT 年度,
		銷售收入,
		利潤
	FROM 收入利潤
	WHERE 1=1
		${if(len(year)==0,"","AND 年度="+year+"")}
		) B
ON A.年度=B.年度]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="Cht_AvgWage" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="year"/>
<O>
<![CDATA[2010]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemoTW]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT A.年度,
	ROUND(工資合計/人數,2) AS 人均工資 
FROM (
	SELECT 年度,
		SUM(合計) AS 工資合計 
	FROM 崗位工資統計
	WHERE 1=1 
		${if(len(year)==0,"","AND 年度="+year+"")} 
	GROUP BY 年度
	) a
JOIN (
 	SELECT ${year} AS 年度,
 		COUNT(ID) AS 人數 
 	FROM 人員花名冊
 	) B
ON A.年度 = B.年度]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="Cht_HumanCostRatio" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="year"/>
<O>
<![CDATA[2010]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemoTW]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT A.年度,ROUND(金額/總金額,2) AS 人力成本占比
FROM (
	SELECT 年度,
		SUM(金額) as 總金額 
	FROM 各類成本記錄
	WHERE 1=1 
		${if(len(year)==0,"","AND 年度="+year+"")}
	GROUP BY 年度
	) A

JOIN (
	SELECT 年度,
		金額 
	FROM 各類成本記錄
	WHERE 項目='人力成本' 
	${if(len(year)==0,"","AND 年度="+year+"")}
	) B
ON A.年度=B.年度]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
